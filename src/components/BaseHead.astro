---
export interface Props {
  title: string
  fav: string
}
const { title = 'Generic Page', fav = '' } = Astro.props
---

<head>
  <meta charset="utf-8" />
  <link
    rel="icon"
    type="image/svg+xml"
    href={fav}
  />
  <meta
    name="viewport"
    content="width=device-width"
  />
  <meta
    name="generator"
    content={Astro.generator}
  />
  <title>{title}</title>
</head>
